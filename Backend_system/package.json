{
  "name": "chat-system-backend",
  "version": "1.0.0",
  "description": "Backend for Chat System - MEAN Stack (Phase 2)",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "peerjs": "node peerjs-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run peerjs\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:routes": "jest --testPathPattern=\"**/routes/**/*.test.ts\"",
    "test:services": "jest --testPathPattern=\"**/services/**/*.test.ts\"",
    "test:unit": "jest --testPathPattern=\"**/__tests__/**/*.test.ts\"",
    "test:integration": "jest --testPathPattern=\"**/integration/**/*.test.ts\"",
    "test:debug": "jest --verbose --detectOpenHandles",
    "test:report": "jest --coverage --coverageReporters=html",
    "test:validate": "node src/__tests__/test-runner.js validate",
    "seed:fake": "ts-node src/__tests__/fixtures/fake-data/seed-database.ts",
    "seed:test": "cross-env NODE_ENV=test ts-node src/__tests__/fixtures/fake-data/seed-database.ts",
    "seed:sample": "ts-node src/scripts/seed-sample-data.ts",
    "seed:mongodb": "ts-node src/services/database.seeder.ts",
    "migrate:mongodb": "ts-node src/scripts/migrate-to-mongodb.ts"
  },
  "keywords": [
    "chat",
    "websocket",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "David Nguyen",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "peer": "^1.0.2",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
