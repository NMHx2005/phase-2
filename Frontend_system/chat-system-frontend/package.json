{
  "name": "chat-system-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage",
    "test:watch": "ng test --watch",
    "test:unit": "ng test --include=\"**/*.spec.ts\"",
    "test:components": "ng test --include=\"**/components/**/*.spec.ts\"",
    "test:services": "ng test --include=\"**/services/**/*.spec.ts\"",
    "test:auth": "ng test --include=\"**/auth/**/*.spec.ts\"",
    "test:chat": "ng test --include=\"**/chat/**/*.spec.ts\"",
    "test:admin": "ng test --include=\"**/admin/**/*.spec.ts\"",
    "test:client": "ng test --include=\"**/client/**/*.spec.ts\"",
    "test:shared": "ng test --include=\"**/shared/**/*.spec.ts\"",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:headless": "cypress run --headless",
    "e2e:ci": "cypress run --browser chrome --headless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/socket.io-client": "^1.4.36",
    "file-saver": "^2.0.5",
    "peerjs": "^1.5.5",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.0",
    "@cypress/schematic": "^4.1.4",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "cypress": "^15.3.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
